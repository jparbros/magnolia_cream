<%= render :partial => '/admin/merchandise/sub_header'%>

<h2>New Pick of the week</h2>


  <div id="article-sidebar" class="sidebar">
    <%= form_for @pick_of_week, :url  => admin_merchandise_pick_of_weeks_path,
                            :html => {:class => ''}  do |form| %>
      <%= render :partial => 'form', :locals => {:form => form } %>
      <div class=" generic_submit_button">
        <%= submit_tag "Create",
                        :class  => "button",
                        :id     => "create_product_button" %>
      </div>
    <% end %>
  </div>

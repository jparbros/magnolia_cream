.pick-of-week-container {
  .pick-of-week {
    background: url('/assets/prototype-bg.png') no-repeat;
    width: 1262px;
    height: 622px;
    margin: 30px auto 0 auto;
  
    .title {
      position: relative;
      top: -30px;
    }
  
    .pick-of-week-name {
      font-family: "Lemondrop";
      font-size: 26px;
      color: #fff;
      margin: 0 0 0 50px;
    }
  
    .pick-of-week-quoete {
      font-family: "Lemondrop";
      font-size: 26px;
      color: #fff;
      margin: 0 0 0 100px;
    }
  
    .pick-of-week-properties {
      text-align: center;
      margin: 20px 0 0 0;

      .pick-of-week-property {
        display: inline-block;
        margin: 0 10px 0 0;

        span {
          font-family: "Brush Script";
          color: #f7f5cb;
          font-size: 24pt;
          text-shadow: 4px 0 0 #8b1b3f, -4px 0 0 #8b1b3f, 0 4px 0 #8b1b3f, 0 -4px 0 #8b1b3f, 2px 2px #8b1b3f, -2px -2px 0 #8b1b3f, 2px -2px 0 #8b1b3f, -2px 2px 0 #8b1b3f;
          margin: -50px 0 0 50px;
          display: block;
        }
      }
    
      img {
        width: 167px;
        height: 167px;
      }
    }
  }

  .pick-of-week-bottom {
    .pick-of-week-actions {
      width: 262px;
      float: right;
    }
  }
  
  .create-yours {
    position: relative;
    top: -650px;
    float: right;
    right: -215px;
  }
}


.feeling-happy {
  font-family: "Brush Script";
  color: #f7f5cb;
  font-size: 36pt;
  text-shadow: 4px 0 0 #8b1b3f, -4px 0 0 #8b1b3f, 0 4px 0 #8b1b3f, 0 -4px 0 #8b1b3f, 2px 2px #8b1b3f, -2px -2px 0 #8b1b3f, 2px -2px 0 #8b1b3f, -2px 2px 0 #8b1b3f;
}